{
	"name": "Load_ AdventureWorksLT",
	"properties": {
		"activities": [
			{
				"name": "ForEach_sui",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.cw_items",
						"type": "Expression"
					},
					"activities": [
						{
							"name": "Copy_sui",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [
								{
									"name": "Source",
									"value": "SalesLT.@{item().source.table}"
								},
								{
									"name": "Destination",
									"value": "staging/AdventureWorks/{year}/{month}/{day}/@{item().destination.fileName}"
								}
							],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"partitionOption": "None"
								},
								"sink": {
									"type": "DelimitedTextSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "DelimitedTextWriteSettings",
										"quoteAllText": true,
										"fileExtension": ".txt"
									}
								},
								"enableStaging": false,
								"validateDataConsistency": false
							},
							"inputs": [
								{
									"referenceName": "SourceDataset_sui",
									"type": "DatasetReference",
									"parameters": {
										"cw_table": "@item().source.table"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "DestinationDataset_sui",
									"type": "DatasetReference",
									"parameters": {
										"cw_fileName": "@item().destination.fileName"
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"cw_items": {
				"type": "Array",
				"defaultValue": [
					{
						"source": {
							"table": "Address"
						},
						"destination": {
							"fileName": "Address@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					},
					{
						"source": {
							"table": "Customer"
						},
						"destination": {
							"fileName": "Customer@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					},
					{
						"source": {
							"table": "CustomerAddress"
						},
						"destination": {
							"fileName": "CustomerAddress@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					},
					{
						"source": {
							"table": "Product"
						},
						"destination": {
							"fileName": "Product@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					},
					{
						"source": {
							"table": "ProductCategory"
						},
						"destination": {
							"fileName": "ProductCategory@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					},
					{
						"source": {
							"table": "ProductModel"
						},
						"destination": {
							"fileName": "ProductModel@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					},
					{
						"source": {
							"table": "SalesOrderDetail"
						},
						"destination": {
							"fileName": "SalesOrderDetail@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					},
					{
						"source": {
							"table": "SalesOrderHeader"
						},
						"destination": {
							"fileName": "SalesOrderHeader@concat(formatDatetime(utcnow(),'dd-MM-yyy'),'.csv')"
						}
					}
				]
			}
		},
		"annotations": []
	}
}